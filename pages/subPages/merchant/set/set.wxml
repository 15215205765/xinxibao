ose" src="/static/images/merchant/close.png" data-event-opts="{{[['tap',[['closePreview',[3,'$0'],[[['type3Url','',index]]]]]]]}}" bindtap="__e"></image></view></block><view data-event-opts="{{[['tap',[['uploadFiles',[3,6]]]]]}}" class="userImgUpdata" bindtap="__e"><image src="/static/images/merchant/addImg.svg"></image></view></view></view></view></view><view data-event-opts="{{[['tap',[['saveUserInfo',['$event']]]]]}}" class="submitButtom" bindtap="__e"><label class="_span">提交</label></view></view></view>