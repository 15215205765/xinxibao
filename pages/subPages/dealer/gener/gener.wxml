<view><block wx:if="{{!loadlogo}}"><loadlogo vue-id="1" bind:__l="__l"></loadlogo></block><block wx:if="{{loadlogo}}"><view class="container"><view class="header"><view class="tabBar b-f dis-flex"><block wx:for="{{tabBar}}" wx:for-item="item" wx:for-index="index"><view data-event-opts="{{[['tap',[['curtabBar',['$0'],[[['tabBar','',index,'typeTab']]]]]]]}}" class="{{['tabBar-item p-r f-24 col-9 t-c',(currentTab===item.typeTab)?'tabBar-active-text':'']}}" bindtap="__e">{{item.title+''}}<view class="{{[(currentTab===item.typeTab)?'active':'']}}"></view></view></block></view></view><view class="content"><view class="search-main m-top-btm40"><view class="client-total m-btm30"><view class="f-24 col-9 m-right20">{{"退款 "+refunded_order+" 已结算 "+settled_order+" 未结算 "+unsettled_order}}</view></view><view class="search-box dis-flex flex-y-center"><view class="search-input p-left-right-30 "><input class="f-24" type="number" value="{{searchText}}" placeholder="输入订单号查询" placeholder-style="color:#999999;" data-event-opts="{{[['input',[['__set_model',['','searchText','$event',[]]]]]]}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="search-btn col-f f-24 t-c" bindtap="__e">搜索</view></view></view><block wx:if="{{!!list&&list.length>0}}"><block><view class="order-list-box"><scroll-view class="scroll-view" scroll-y scroll-top="{{slide_Top}}" data-event-opts="{{[['scrolltolower',[['ReachBottom',['$event']]]],['scroll',[['scrolly',['$event']]]]]}}" bindscrolltolower="__e" bindscroll="__e"><view class="order-list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index"><view class="order-item b-f bor-radius-10upx padding-box-all m-btm30"><view class="user-detail p-r dis-flex flex-y-center"><view class="user-avatar m-right30" style="{{'background-image:'+('url('+item.avatar+')')+';'}}"></view><view class="user-nickname"><view class="f-24 col-9 scale-us m-btm10">下单用户</view><view class="f-24 col-3">{{item.nickname}}</view><view class="order-status f-24">{{item.status==='0'?orderStatus[3]['title']:item.status==='1'?orderStatus[0]['title']:item.status==='2'?orderStatus[1]['title']:orderStatus[2]['title']}}</view></view></view><view class="order-goods dis-flex border-line border-bottom"><view class="goods-image bor-radius-10upx m-right30" style="{{'background-image:'+('url('+item.goods_logo+')')+';'}}"></view><view class="goods-detail"><view class="goods-title twolist-hidden col-3 f-24">{{item.goods_name}}</view><view class="f-24 col-9 goods-now-price">{{"实付金额 ￥"+item.orderprice}}</view><view class="f-28 commission-price">{{dealer_Info.yjtext+" ￥"+item.commission}}</view></view></view><view class="order-detail"><view class="order-number dis-flex flex-y-center flex-x-between"><view class="col-9 f-24">{{"订单号："+item.orderno}}</view><view data-event-opts="{{[['tap',[['copy_orderno',['$0'],[[['list','',index,'orderno']]]]]]]}}" class="copy-btn t-c col-9 f-24" bindtap="__e">复制</view></view><view class="f-24 col-9 m-top-btm20">{{"支付时间："+item.paytime}}</view><block wx:if="{{!!item.use_time&&item.use_time.length>0}}"><view class="f-24 col-9">{{(item.consumption_type===1?'收货时间':item.consumption_type===2?'核销时间':'使用时间')+"："+item.use_time[0]}}</view></block></view></view></block></view><load-more vue-id="2" isMore="{{loadingMore}}" bind:__l="__l"></load-more></scroll-view></view></block></block><block wx:else><block><none-mores vue-id="3" bind:__l="__l"></none-mores></block></block></view></view></block></view>