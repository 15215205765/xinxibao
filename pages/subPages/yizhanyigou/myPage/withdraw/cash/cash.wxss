page{
	background: #f8f8f8;
}
.withdraw{
	padding: 0 20rpx;
	padding-top: 36rpx;
}

.title{
	font-size: 28rpx;
	color: #999;
}
.withdraw .withdraw-money{
	background: #fff;
	padding: 24rpx;
	margin-top: 22rpx;
}
.withdraw .withdraw-money .money{
	font-size: 48rpx;
	color: #333;
	padding-bottom: 18rpx;
	border-bottom: 1px solid #DCDCDC;
}
.withdraw .withdraw-money .have{
	font-size: 28rpx;
	color: #999;
	margin-top: 12rpx;
}
.withdraw .explain{
	width: 100%;
	text-align: right;
	font-size: 28rpx;
	color: #333;
	margin-top: 18rpx;
	
}
.withdraw-app{
	padding: 0 20rpx;
	margin-top: 16rpx;
}
.withdraw-app image{
	width: 100%;
	height: 88rpx;
	margin-top: 22rpx;
}

.personal {
}

.personal .item {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: #fff;
  padding: 24rpx 20rpx;
  font-size: 24rpx;
  color: #333;
  border-bottom: 1px solid #ececec;
}




.item .left {
}

.item .right {
	display: flex;
	align-items: center;
}
.item .right .code{
	width: 166rpx;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	border-left: 1px solid #efefef;.
	margin-left: 10rpx;
	color: #fff;
	height:48rpx;
	border-radius: 24rpx;
	background: linear-gradient(to right,#FD3D34,#FD6534)
}

.code-box input{
	text-align: right;
	margin-right: 30rpx;
}
.code-box .active{
	color: rgba(253, 52, 52, 1);
}
.item .right image {
	width: 11rpx;
	height: 20rpx;
	margin-left: 30rpx;
}
.item .right input {
  text-align: right;
  color: #999999;
}

.submit {
  margin-top: 20rpx;
  width: 600rpx;
  height: 88rpx;
  font-size: 34rpx;
  background: #FF9800;
  color: #fff;
}
/* 温馨提示 */
.tips{
	padding:0 20rpx;
	margin-top: 20rpx;
}
.tips .top{
	display: flex;
align-items: center;
}
.tips .top .title{
	width: 20%;
	font-size: 28rpx;
	color: #333;
}
.tips .top .line{
	width: 100%;
	height: 1px;
	background:#DCDCDC;
}
.tips .content{
	font-size: 24rpx;
	color: #999;
	margin-top: 13rpx;
}
.tips .content view{
	line-height: 40rpx;
}
.support-title{
	font-size:32rpx;
	color:rgba(102,102,102,1);
	padding: 30rpx 24rpx;
}

.withdrawal-money-box{
	display: flex;
	justify-content: center;
	align-items: center;
	color: #fff;
	height: 220rpx;
	font-size:32rpx;
	background: linear-gradient(to bottom,#FD7034,#FD4D34);
}
.withdrawal-mone{
	display: flex;
	justify-content: center;
	align-items: center;
}
.withdrawal-money text{
	font-size:60rpx;
}
.withdrawal-branch{
	background: #F6271C;
	display: flex;
	align-items: center;
}
.withdrawal-branch .item{
	width: 33.33%;
	color: #fff;	
	text-align: center;
	padding: 28rpx 0;
}

.withdrawal-branch .item view{
	font-size: 20rpx;
}
.withdrawal-branch .item view>text{
	font-size: 32rpx;
}

.withdrawal-branch .item>text{
	font-size:28rpx;
	color:rgba(255,255,255,1);
}
.withdrawal-branch .line{
	width:0px;
	height:56rpx;
	border:1px solid rgba(255,255,255,1);
	opacity:0.8;
}