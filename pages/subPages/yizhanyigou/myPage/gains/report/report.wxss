page {
	height: 100%;
	width: 100%;
}

.page {
	height: 100%;
	width: 100%;
}

.header {
	height: 240rpx;
	width: 100%;
	text-align: center;
	padding-top: 72rpx;
	position: relative;
	background: linear-gradient(229deg, rgba(253, 114, 52, 1) 0%, rgba(253, 52, 52, 1) 100%);
}

.header .money {
	display: flex;
	justify-content: center;
	align-items: flex-end;
	color: #fff;
}

.header .money .money-unit {
	font-size: 36rpx;
}

.header .money .money-integer {
	font-size: 64rpx;
	font-weight: bold;
	position: relative;
	bottom: 0;
	margin-bottom: -10rpx;

}

.header .money .money-decimal {
	font-size: 36rpx;
	font-weight: bold;
}

.header .text {
	font-size: 28rpx;
	color: #fff;
	margin-top: 32rpx;
}

.gains-list {}

.gains-item .gains-top {
	display: flex;
	/* padding: 26rpx 20rpx; */
	height: 80rpx;
	padding:0 20rpx;
	align-items: center;
	justify-content: space-between;
	background: #F8F8F8;
}

.gains-top .gains-name {
	font-size: 28rpx;
	color: #333;
	font-weight: bold;
	display: flex;
	align-items: center;
}
.gains-top .gains-name .raduis{
	width:20rpx;
height:20rpx;
border-radius:50%;
margin-right: 18rpx;
}
.gains-top .right{
	display: flex;
	align-items: center;
}
.gains-money {
	display: flex;
	font-size: 20rpx;
	align-items: flex-end;
	color: #FD3434;
}

.gains-money .money-integer {
	font-size: 32rpx;
	font-weight: bold;
	position: relative;
	bottom: 0;
	margin-bottom: -2rpx;

}

.gains-money .money-decimal {
	font-weight: bold;
}
.right image{
	height: 20rpx;
	width: 11rpx;
	margin-left: 34rpx;
}
.gains-source {
	background: #fff;
	display: flex;
	align-items: center;
}
.gains-source-item{
	padding: 25rpx 0;
	width: 33.33%;
	display: flex;
	flex-flow: column;
	align-items: center;
}
.gains-source-money {
	display: flex;
	font-size: 20rpx;
	align-items: flex-end;
	color: #333;
}

.gains-source-money .money-integer {
	font-size: 32rpx;
	font-weight: bold;
	position: relative;
	bottom: 0;
	margin-bottom: -2rpx;
	
}

.gains-source-money .money-decimal {
	font-weight: bold;
}
.gains-source-item .gains-source-name{
	font-size: 24rpx;
	color: #999;
	margin-top: 14rpx;

}
.gains-source-line{
	width:0px;
height:47rpx;
border:1px solid rgba(220,220,220,1);
}
.all{
  width: 50%;
}
.explain{
	position: absolute;
	right: 36rpx;
	top:50%;
	transform: translateY(-50%);
	font-size: 28rpx;
	color: #fff;
}
.withdrawal-money-box{
	display: flex;
	flex-flow: column;
	justify-content: center;
	align-items: center;
	color: #fff;
	height: 215rpx;
	font-size:32rpx;
	background: linear-gradient(to bottom,#FD7034,#FD4D34);
	position: relative;
}
.withdrawal-money-box .question-mark{
	width: 30rpx;
	height: 30rpx;
	position: absolute;
	right: 20rpx;
	top: 28rpx;
}
.withdrawal-mone{
	display: flex;
	justify-content: center;
	align-items: center;
}
.withdrawal-money{
	font-size:64rpx;
	font-weight: bold;
	display: flex;
	align-items: center;
}
.withdrawal-branch{
	background: #F6271C;
	display: flex;
	align-items: center;
}
.withdrawal-branch .item{
	width: 33.33%;
	color: #fff;	
	text-align: center;
	/* padding: 28rpx 0; */
	height: 132rpx;
	display: flex;
	flex-flow: column;
	justify-content: center;
	align-items: center;
}

.withdrawal-branch .item view{
	font-size: 20rpx;
}
.withdrawal-branch .item view>text{
	font-size: 32rpx;
}

.withdrawal-branch .item>text{
	font-size:28rpx;
	color:rgba(255,255,255,1);
}
.withdrawal-branch .line{
	width:0px;
	height:56rpx;
	border:1px solid rgba(255,255,255,1);
	opacity:0.8;
}
.hidden-box{
	width: 44rpx;
	height: 44rpx;
	position: relative;
	margin-left: 10rpx;
}
.hidden-img{
	width: 185rpx;
	height: 24rpx;
}
.hidden-box image{
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	position: absolute;
}