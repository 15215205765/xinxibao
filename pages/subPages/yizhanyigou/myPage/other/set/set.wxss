.list{
	padding-top: 20rpx;
}
.list .item{
	padding:0 20rpx;
	height: 88rpx;
	font-size: 28rpx;
	color: #333;
	background: #fff;
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-bottom: 20rpx;
}
.list .item image{
	width: 11rpx;
