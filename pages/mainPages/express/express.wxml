<view class="page"><block wx:if="{{!loadlogo}}"><loadlogo vue-id="1" bind:__l="__l"></loadlogo></block><block wx:if="{{loadlogo}}"><view><view class="header m-btm20"><view class="goods_detail dis-flex m-btm20 padding-box-all b-f"><view class="goods_image" style="{{('background-image:url('+loggDataDetail.goods_logo+')')}}"></view><view class="goods_text dis-flex flex-dir-column flex-x-between"><view class="f-28 col-3 m-btm20">{{loggDataDetail.goods_name}}</view><block wx:if="{{!!loggDataDetail.spec}}"><view class="col-9 f-24">{{"规格:"+loggDataDetail.spec}}</view></block><view class="col-9 f-24">{{"数量："+loggDataDetail.num+"件"}}</view></view></view><view class="loggDataDetail-info m-btm20 padding-box-all b-f"><view class="f-24 col-9 m-btm20">快递公司:<text class="col-3 m-left10">{{loggDataDetail.express_name}}</text></view><view class="f-24 col-9">物流单号:<text class="col-3 m-left10">{{loggDataDetail.express_no}}</text></view></view></view><block wx:if="{{loggDataDetail.is_have===1}}"><block><view class="logis bor-radius-10 b-f m-btm20"><view class="p-right-30"><view class="logis-list p-top-bom-40"><view class="loggis-section"><block wx:for="{{loggDataDetail.list}}" wx:for-item="items" wx:for-index="index" wx:key="index"><view><view class="loggis-move"><view class="dis-flex flex-y-center"><block wx:if="{{index===0}}"><block><view class="Icon-bg"><view class="icon-line iconfont icon-check col-f"></view></view></block></block><block wx:else><block><view class="circle-seg"></view></block></block><view class="flex-box "><view class="{{['f-24 m-btm10',index===0?'col-3':'col-9']}}">{{items.AcceptStation}}</view><view class="{{['f-24',index===0?'col-3':'col-9']}}">{{items.AcceptTime}}</view></view></view></view><block wx:if="{{loggDataDetail.list.length>0&&index<loggDataDetail.list.length-1}}"><view class="logg-dotted"></view></block></view></block></view></view></view></view></block></block></view></block></view>